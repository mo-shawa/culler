{
	"name": "culler",
	"version": "0.0.5",
	"description": "Blazingly tiny, 0 dependency package for generating a color.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "tsup index.ts --format cjs,esm --dts",
		"lint": "tsc",
		"dry-run": "npm run build && npm publish --dry-run",
		"prepublishOnly": "tsc && npm run build",
		"changeset-init": "changeset init",
		"changeset": "changeset"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mo-shawa/culler.git"
	},
	"author": "Mahmoud El-Shawa",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/mo-shawa/culler/issues"
	},
	"homepage": "https://github.com/mo-shawa/culler#readme",
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"tsup": "^6.5.0",
		"typescript": "^4.9.5"
	}
}
