{
  "name": "culler",
  "version": "0.7.2",
  "description": "Blazingly tiny, 0 dependency package color utility library.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "release": "npm run build && npx changeset version && npx changeset publish",
    "test": "jest --watchAll --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mo-shawa/culler.git"
  },
  "author": "Mahmoud El-Shawa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mo-shawa/culler/issues"
  },
  "homepage": "https://github.com/mo-shawa/culler#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/jest": "^29.4.0",
    "css-color-types": "^0.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "culler",
    "ts",
    "TypeScript",
    "utility",
    "library",
    "css",
    "color",
    "colour",
    "convert",
    "generate",
    "color generator",
    "colour generator",
    "rgb",
    "rgba",
    "hex",
    "hexadecimal"
  ]
}
